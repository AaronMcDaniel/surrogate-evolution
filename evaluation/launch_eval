#!/bin/bash
#SBATCH --job-name=eval_job
#SBATCH --nodes=1
#SBATCH --gres=gpu:1
#SBATCH --cpus-per-task=8
#SBATCH --mem=16GB
#SBATCH --time=1:00:00
#SBATCH --output=evaluation.log
#SBATCH --error=evaluation_error.log

module load anaconda3/2023.07
module load cuda/12.2.2
conda activate tv1
cd evaluation
# Need encoded genome, current gen, current individual number, number of epochs, output directory
python eval.py --genome $1 --config $2
conda deactivate
